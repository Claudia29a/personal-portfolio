{
  "builds": [
    {
      "src": "portfolio/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/portfolio/browser" }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot))$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)$",
      "dest": "/index.html"
    }
  ]
}
