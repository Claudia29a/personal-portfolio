{
  "buildCommand": "cd portfolio && npm install && npm run build:static",
  "outputDirectory": "portfolio/dist/portfolio/browser",
  "installCommand": "npm install --prefix ./portfolio",
  "routes": [
    {
      "src": "/(.*\\.(css|js|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
